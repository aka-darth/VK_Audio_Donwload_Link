{
 "name": "VK audio extension by Darth",
	"manifest_version": 2,
	"version": "3.0.33",
	"description": "Плагин добавляет кнопки скачать к аудиозаписям вконтакте",
	"permissions":
	[
		"storage",
		"tabs",
		"http://vk.com/*",
		"https://vk.com/*"
	],
	"content_scripts":
	[
		{
			"matches": [
				"http://vk.com/*",
				"https://vk.com/*"
			],
			"js": ["js.js"]
		}
	],
	"browser_action": {
		"default_icon":"logo.png",
		"default_popup":"popup.html"
	}
}
